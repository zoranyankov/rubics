<header>
    <ul>
        <li><img class="logo" src="/images/logo.png"></li>
        <li><a href="/">Browse</a></li>
        <li><a href="/about">About</a></li>
        {{#if isAuthenticated}}
        <li><a href="/cubes/create">Add a Cube</a></li>
        <li><a href="/accessories/create">Add Accessory</a></li>
        <li><a href="/auth/logout">Logout</a></li>
        <li class="wellcome">
            <h4>User {{loggedUser}} </br> is logged</h4>
        </li>
        {{else}}
        <li><a href="/auth/login">Login</a></li>
        <li><a href="/auth/register">Register</a></li>
        <li class="wellcome">
            <h4>Guest </br> view</h4>
        </li>
        {{/if}}
    </ul>
</header>
{{#if errors}}
<div id="notifications">
{{#each errors}}
    <p id="notification-message">{{message}}</p>
{{/each}}
</div>
{{/if}}